<script src="/public/js/login.js"></script>

   </head>
   <body>
   	<br>
   	<br>
   	<center><h1>Hospital Login</h1></center>
    
   	<div class="login-page">
       <form class="register-form" action="/hospitallogin" method="POST">
         <div class="form">
           <input type="text" name="adminName" id="adminName" placeholder="Hospital UserName"/>
           <input type="password" name="adminPassword" id="adminPassword" placeholder="Password"/>
           <p><%=msg%></p>
           
            <button type="submit">Login</button> 
           
            </div>
          </form>
          
</div>

   </body>
  </html>